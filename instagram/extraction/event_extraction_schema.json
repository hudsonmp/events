{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "title": "Event Extraction Schema",
  "description": "Schema for extracting event information from Instagram posts",
  "properties": {
    "name": {
      "type": "string",
      "description": "The name/title of the event"
    },
    "start_datetime": {
      "type": "string",
      "format": "date-time",
      "description": "Start date and time of the event in ISO 8601 format"
    },
    "end_datetime": {
      "type": ["string", "null"],
      "format": "date-time",
      "description": "End date and time of the event in ISO 8601 format (optional)"
    },
    "address": {
      "type": ["string", "null"],
      "description": "Physical address where the event takes place"
    },
    "location_name": {
      "type": ["string", "null"],
      "description": "Name of the venue or location"
    },
    "description": {
      "type": ["string", "null"],
      "description": "Detailed description of the event"
    },
    "is_all_day": {
      "type": "boolean",
      "default": false,
      "description": "Whether the event lasts all day"
    },
    "type": {
      "type": "string",
      "enum": ["in-person", "virtual", "hybrid"],
      "default": "in-person",
      "description": "Type of event delivery"
    },
    "categories": {
      "type": "array",
      "description": "List of categories that apply to this event",
      "items": {
